{
	"path": {
		"src/main/sass/*.scss": {
			"sass.enabled": true,
			"sass.options": {
				"outputDir": "../../assets/css/",
				"imagePath": null,
				"sourceComments": true,
				"outputStyle": "nested"
			},
			"linting.collapsed": true
		}
	},
    "jwolfe.file-tree-exclude.list": [
		"node",
		"node_modules",
		"bower_components",
		".git",
		".sass-cache",
		"PhantomJS*",
        "ceylon"
    ]
}
